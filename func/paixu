<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Document</title>
</head>
<body>
	<script>
		function quick(arr){
			var num = Math.floor(arr.length/2);
			var numvalue = arr.splice(num,1);
			if(arr.length == 1){
				return arr;
			}else{
				var left=[];
				var right=[];
				for(var i=0;i<arr.length;i++){
					if(arr[i]<numvalue){
						left.push[arr[i]];
					}else{
						right.push[arr[i]];
					}
				}
			}
			return quick(left)+concat([numvalue],quick(right));
		}
		alert(quick([12,21,23,12,25,235]));
	</script>
	
</body>
</html>
